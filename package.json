{
  "name": "blog-project",
  "version": "1.0.0",
  "description": "Blog Project",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"tsc-alias -w\" \"nodemon -q dist/index.js\"",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "console": "npm i -g && console"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "bin": {
    "console": "./dist/modules/console/index.js"
  },
  "keywords": [],
  "author": "nathan",
  "license": "MIT",
  "dependencies": {
    "@types/cheerio": "^0.22.31",
    "axios": "^0.21.4",
    "cheerio": "^1.0.0-rc.12",
    "clear": "^0.1.0",
    "commander": "^9.4.1",
    "cron": "^2.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.5",
    "pg": "^8.8.0",
    "sanitize-html": "^2.7.3",
    "sequelize": "^6.26.0",
    "sequelize-cli": "^6.5.2",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/clear": "^0.1.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.10",
    "@types/pg": "^8.6.5",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "tsc-alias": "^1.8.1",
    "typescript": "^4.9.3"
  },
  "_moduleAliases": {
    "@": "dist",
    "@type": "dist/type",
    "@module": "dist/modules",
    "@user": "dist/modules/user",
    "@utils": "dist/utils",
    "@base": "dist/base",
    "@exception": "dist/exception",
    "@interface": "dist/interface"
  }
}
